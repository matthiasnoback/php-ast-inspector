{
    "name": "matthiasnoback/php-ast-inspector",
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.1",
        "nikic/php-parser": "^4.14"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^11.1",
        "symplify/coding-standard": "^11.1",
        "phpunit/phpunit": "^9.5.21",
        "phpstan/phpstan": "^1.8.2",
        "rector/rector": "^0.14.2"
    },
    "autoload": {
        "psr-4": {
            "PhpAstInspector\\": "src/PhpAstInspector/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Utils\\PHPStan\\": "utils/PHPStan/src",
            "Utils\\PHPStan\\Tests\\": "utils/PHPStan/tests"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "bin": ["composer-bin/ast-inspect"]
}
